{
  "manifest_version": 3,
  "name": "Smart Web Collector",
  "version": "1.0.0",
  "description": "Intelligent web data collection, enrichment, and export.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "contextMenus",
    "notifications",
    "identity"
  ],
  "key": "",
  "oauth2": {
    "client_id": "746562803104-gnmu4ieq4e60gbo647gvsrpgnjm7lt46.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*",
    "file:///*"
  ],
  "optional_permissions": [
    "https://sheets.googleapis.com/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "magic.js",
        "content.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "commands": {
    "toggle-magic-bar": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "Toggle Magic Extract Bar"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "lib/pdf.min.js",
        "lib/pdf.worker.min.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
  }
}